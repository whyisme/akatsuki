{
  "name": "clearcase",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "An Atom Plugin for users of Clearcase",
  "activationCommands": {
    "atom-workspace": [
      "clearcase:make",
      "clearcase:upload",
      "clearcase:uploadTo",
      "clearcase:newHost",
      "clearcase:editHost",
      "clearcase:checkout",
      "clearcase:undoCheckout",
      "clearcase:checkin",
      "clearcase:refreshFileStatus"
    ]
  },
  "deserializers": {
    "ProjectInfo": "restoreProjectInfo"
  },
  "engines": {
    "atom": ">=1.2.3 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "fs-plus": ">=2.2.3",
    "iconv-lite": "^0.4.15",
    "os-locale": "^2.0.0"
  }
}
